{
  "all": true,
  "include": [
    "out/**/*.js"
  ],
  "exclude": [
    "out/test/**",
    "out/**/*.test.js"
  ],
  "reporter": [
    "text",
    "html",
    "lcov"
  ],
  "report-dir": "./coverage",
  "check-coverage": true,
  "lines": 80,
  "functions": 80,
  "branches": 70,
  "statements": 80
}